class Solution:
    def searchRange(self, nums: List[int], target: int) -> List[int]:
                
        if target not in nums:
            return [-1, -1]
        def left():
            left = 0
            right = len(nums) - 1
            res = 0
            while left <= right:
                mid = (left + right) //2
                if nums[mid] == target:
                    res = mid
                    right = mid - 1
                elif nums[mid] < target:
                    left = mid + 1
                else:
                    right = mid - 1
            return res

        def right():
            left = 0
            right = len(nums) - 1
            res = 0
            while left <= right:
                mid = (left + right) //2
                if nums[mid] == target:
                    res = mid
                    left = mid + 1
                elif nums[mid] < target:
                    left = mid + 1
                else:
                    right = mid - 1
            return res

        
        return [left(), right()]

